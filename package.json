{
  "name": "rupeerush",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:web": "turbo dev --filter=\"./apps/user-app\"",
    "dev:hook": "turbo dev --filter=\"./apps/bankWebhook\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@9.7.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
